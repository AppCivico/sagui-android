<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="cpv_progress" format="float"/>
        <attr name="cpv_max" format="integer"/>

        <attr name="cpv_finished_color" format="color"/>
        <attr name="cpv_finished_end_color" format="color"/>
        <attr name="cpv_unfinished_color" format="color"/>

        <attr name="cpv_finished_stroke_width" format="dimension"/>
        <attr name="cpv_unfinished_stroke_width" format="dimension"/>

        <attr name="cpv_text_size" format="dimension"/>
        <attr name="cpv_text_color" format="color"/>

        <attr name="cpv_prefix_text" format="string"/>
        <attr name="cpv_suffix_text" format="string"/>
        <attr name="cpv_text" format="string"/>

        <attr name="cpv_background_color" format="color"/>

        <attr name="cpv_inner_bottom_text" format="string"/>
        <attr name="cpv_inner_bottom_text_size" format="dimension"/>
        <attr name="cpv_inner_bottom_text_color" format="color"/>

        <attr name="cpv_circle_starting_degree" format="integer"/>
        <attr name="cpv_show_text" format="boolean"/>
        <attr name="cpv_inner_drawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension"/>
        <attr name="civ_border_color" format="color"/>
        <attr name="civ_fill_color" format="color"/>
        <attr name="civ_disable_circular" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CheckableImageView">
        <attr name="cciv_textColor" format="color"/>
        <attr name="cciv_textSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SquareLinearLayout">
        <attr name="srl_adjustDimension" format="enum">
            <enum name="height" value="0" />
            <enum name="width" value="1" />
        </attr>
    </declare-styleable>
</resources>